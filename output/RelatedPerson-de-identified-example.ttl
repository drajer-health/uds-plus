@prefix fhir: <http://hl7.org/fhir/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# - resource -------------------------------------------------------------------

<http://fhir.org/guides/hrsa/uds-plus/RelatedPerson/de-identified-example> a fhir:RelatedPerson ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "de-identified-example"] ; # 
  fhir:meta [
     fhir:profile ( [
       fhir:v "http://fhir.org/guides/hrsa/uds-plus/StructureDefinition/de-identified-uds-plus-relatedperson"^^xsd:anyURI ;
       fhir:link <http://fhir.org/guides/hrsa/uds-plus/StructureDefinition/de-identified-uds-plus-relatedperson>
     ] )
  ] ; # 
  fhir:patient [
     fhir:link <http://fhir.org/guides/hrsa/uds-plus/Patient/de-identified-example> ;
     fhir:reference [ fhir:v "Patient/de-identified-example" ]
  ] ; # 
  fhir:relationship ( [
     fhir:coding ( [
       fhir:system [ fhir:v "http://terminology.hl7.org/CodeSystem/v3-RoleCode"^^xsd:anyURI ] ;
       fhir:code [ fhir:v "FAMMEMB" ] ;
       fhir:display [ fhir:v "family member" ]
     ] )
  ] ) . # 

