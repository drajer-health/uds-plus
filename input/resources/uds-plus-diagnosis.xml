        <element id="Condition.recorder.display">
            <path value="Condition.recorder.display"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Condition.asserter.display">
            <path value="Condition.asserter.display"/>
            <min value="0"/>
