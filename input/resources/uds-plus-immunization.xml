        </element>
        <element id="Immunization.primarySource">
            <path value="Immunization.primarySource"/>
            <min value="1"/>
            <max value="1"/>
            <mustSupport value="true"/>
        </element>
        <element id="Immunization.expirationDate">
            <path value="Immunization.expirationDate"/>
            <min value="0"/>
            <max value="1"/>
            <constraint>
		        <key value="uds-plus-imm-3"/>
		        <severity value="error"/>
		        <human value="All date data elements should be truncated to a year only for de-identified resources."/>        
		        <expression value="$this is date implies $this.toString().length() = 4" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-imm-3a"/>
		        <severity value="error"/>
		        <human value="All dateTime data elements should have valid year &gt; 1900"/>        
		        <expression value="$this is DateTime implies $this.toString() &gt;= '1900'" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-imm-3b"/>
		        <severity value="error"/>
		        <human value="All dateTime data elements should have valid year &lt; 2050"/>        
		        <expression value="$this is DateTime implies $this.toString() &lt;= '2050'" />
		    </constraint>
            <mustSupport value="false"/>
        </element>
        <element id="Immunization.note">
            <path value="Immunization.note"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Immunization.reaction.date">
            <path value="Immunization.reaction.date"/>
            <min value="0"/>
            <max value="1"/>
            <constraint>
		        <key value="uds-plus-imm-4"/>
		        <severity value="error"/>
		        <human value="All dateTime data elements should be truncated to a year only for de-identified resources."/>        
		        <expression value="$this is dateTime implies $this.toString().length() = 4" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-imm-4a"/>
		        <severity value="error"/>
