<?xml version="1.0" encoding="UTF-8"?>
<StructureDefinition
    xmlns="http://hl7.org/fhir">
    <id value="de-identified-uds-plus-income-observation"/>
    <meta>
        <versionId value="12"/>
        <lastUpdated value="2022-07-12T02:03:11.293+00:00"/>
        <source value="#3LDMUQYYwafwTzqK"/>
    </meta>
    <url value="http://fhir.org/guides/hrsa/uds-plus/StructureDefinition/de-identified-uds-plus-income-observation"/>
    <version value="1.0.0"/>
    <name value="UDSPlusIncome"/>
    <title value="De-Identified UDS Plus Income"/>
    <status value="active"/>
    <experimental value="false"/>
    <publisher value="HL7 Public Health Work Group"/>
    <description value="The UDS+ profile is based on the Observation resource and establishes the core elements, extensions, vocabularies and value sets for representing Income ranges for UDS+ program."/>
    <jurisdiction>
        <coding>
            <system value="urn:iso:std:iso:3166"/>
            <code value="US"/>
            <display value="United States of America"/>
        </coding>
        <text value="United States of America"/>
    </jurisdiction>
    <fhirVersion value="4.0.1"/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="Observation"/>
    <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Observation"/>
    <derivation value="constraint"/>
    <differential>
        <element id="Observation">
            <path value="Observation"/>
            <short value="De-Identified UDS Plus Income"/>
            <definition value="The UDS+ profile is based on the Observation resource and establishes the core elements, extensions, vocabularies and value sets for representing income for UDS+ program."/>
        </element>
         <element id="Observation.id">
            <path value="Observation.id"/>
            <short value="The de-identified Observation identifier created by the health center to send data to UDS+ Data Receiver"/>
            <definition value="De-dentified Observation resource identifier."/>
            <mustSupport value="true"/>
        </element>
        <element id="Observation.text">
            <path value="Observation.text"/>
            <short value="Cannot include any text in the resource as part of the de-identified data."/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.contained">
            <path value="Observation.contained"/>
            <short value="Cannot include any contained resources in the resource as part of the de-identified data."/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.extension:deidentifiedIssuedTime">
            <path value="Observation.extension"/>
            <sliceName value="deidentifiedIssuedTime"/>
            <min value="0"/>
            <max value="1"/>
            <type>
                <code value="Extension"/>
                <profile value="http://fhir.org/guides/hrsa/uds-plus/StructureDefinition/uds-plus-deidentified-instant-extension"/>
            </type>
        </element>
        <element id="Observation.status">
            <path value="Observation.status"/>
            <min value="1"/>
            <max value="1"/>
            <fixedCode value="final"/>
            <mustSupport value="true"/>  
        </element>
        <element id="Observation.code">
            <path value="Observation.code"/>
            <min value="1"/>
            <max value="1"/>
            <patternCodeableConcept>
                <coding>
                    <system value="http://loinc.org"/>
                    <code value="63058-2"/>
                </coding>
            </patternCodeableConcept>
            <mustSupport value="true"/>  
        </element>
        <element id="Observation.subject">
            <path value="Observation.subject"/>
            <min value="1"/>
            <max value="1"/>
            <type>
                <code value="Reference"/>
                <targetProfile value="http://fhir.org/guides/hrsa/uds-plus/StructureDefinition/de-identified-uds-plus-patient"/>
            </type>
            <mustSupport value="true"/>
        </element>
        <element id="Observation.subject.display">
            <path value="Observation.subject.display"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.encounter.display">
            <path value="Observation.encounter.display"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.effective[x]">
            <path value="Observation.effective[x]"/>
            <min value="0"/>
            <max value="1"/>
            <type>
            	<extension
                   	url="http://hl7.org/fhir/StructureDefinition/elementdefinition-type-must-support">
          			<valueBoolean value="true"/>
        		</extension>
            	<code value="dateTime"/>
            </type>
            <type>
            	<code value="Period"/>
            </type>
            <type>
            	<code value="Timing"/>
            </type>
            <constraint>
		        <key value="uds-plus-inc-1"/>
		        <severity value="error"/>
		        <human value="All dateTime data elements should be truncated to a year only for de-identified resources."/>        
		        <expression value="$this is DateTime implies $this.toString().length() = 4" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-1a"/>
		        <severity value="error"/>
		        <human value="All dateTime data elements should have valid year &gt; 1900"/>        
		        <expression value="$this is DateTime implies $this.toString() &gt;= '1900'" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-1b"/>
		        <severity value="error"/>
		        <human value="All dateTime data elements should have valid year &lt; 2030"/>        
		        <expression value="$this is DateTime implies $this.toString() &lt;= '2030'" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-2"/>
		        <severity value="error"/>
		        <human value="All Period.start data elements should be truncated to a year only for de-identified resources."/>        
		        <expression value="$this is Period and Period.start.exists() implies $this.start.toString().length() = 4" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-2a"/>
		        <severity value="error"/>
		        <human value="All Period.start data elements should have valid year &gt; 1900"/>        
		        <expression value="$this is Period and Period.start.exists() implies $this.start.toString() &gt;= '1900'" />
		    </constraint>
		   <constraint>
		        <key value="uds-plus-inc-2b"/>
		        <severity value="error"/>
		        <human value="All Period.start data elements should have valid year &lt; 2030"/>        
		        <expression value="$this is Period and Period.start.exists() implies $this.start.toString() &lt;= '2030'" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-3"/>
		        <severity value="error"/>
		        <human value="All Period.emd data elements should be truncated to a year only for de-identified resources."/>        
		        <expression value="$this is Period and Period.end.exists() implies $this.end.toString().length() = 4" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-3a"/>
		        <severity value="error"/>
		        <human value="All Period.end data elements should have valid year &gt; 1900"/>        
		        <expression value="$this is Period and Period.end.exists() implies $this.end.toString() &gt;= '1900'" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-3b"/>
		        <severity value="error"/>
		        <human value="All Period.end data elements should have valid year &lt; 2030"/>        
		        <expression value="$this is Period and Period.end.exists() implies $this.end.toString() &lt;= '2030'" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-4"/>
		        <severity value="error"/>
		        <human value="All Timing.event data elements should be truncated to a year only for de-identified resources."/>        
		        <expression value="$this is Timing and Timing.event.exists() implies $this.event.count() = 1" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-5"/>
		        <severity value="error"/>
		        <human value="All Timing.event data elements should be truncated to a year only for de-identified resources."/>        
		        <expression value="$this is Timing and Timing.event.exists() implies $this.event.first().toString().length() = 4" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-res-5a"/>
		        <severity value="error"/>
		        <human value="All Timing data elements should have valid year &gt; 1900"/>        
		        <expression value="$this is Timing and Timing.event.exists() implies $this.event.first().toString() &gt;= '1900'" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-res-5b"/>
		        <severity value="error"/>
		        <human value="All Timing data elements should have valid year &lt; 2030"/>        
		        <expression value="$this is Timing and Timing.event.exists() implies $this.event.first().toString() &lt;= '2030'" />
		    </constraint>
            <mustSupport value="true"/>
        </element>
        <element id="Observation.issued">
            <path value="Observation.issued"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="true"/>
        </element>
        <element id="Observation.performer.display">
            <path value="Observation.performer.display"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.value[x]">
            <path value="Observation.value[x]"/>
            <min value="0"/>
            <max value="1"/>
            <type>
                <code value="Range"/>
            </type>
            <type>
                <code value="Quantity"/>
            </type>
            <constraint>
		        <key value="uds-plus-inc-6"/>
		        <severity value="error"/>
		        <human value="Income Ranges should be coded according to the UDS+ requirements to be between 101% and 150% or between 151% and 200%"/>        
		        <expression value="$this is Range and Range.low.exists() and Range.high.exists() implies ((Range.low.value &gt;= 101 and Range.high.value &lt;= 150) or (Range.low.value &gt;= 151 and Range.high.value &lt;= 200))" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-7"/>
		        <severity value="error"/>
		        <human value="Income Ranges should be coded according to the UDS+ requirements when less than 100%"/>        
		        <expression value="$this is Range and Range.low.exists().not() and Range.high.exists() implies (Range.high.value &lt;= 100)" />
		    </constraint>
		    <constraint>
		        <key value="uds-plus-inc-8"/>
		        <severity value="error"/>
		        <human value="Income Ranges should be coded according to the UDS+ requirements when more than 200%, Quantity should include value(200), code(%) and comparator(>)"/>        
		        <expression value="$this is Quantity implies ($this.value &gt;= 200 and $this.code = '%' and $this.comparator = '>')" />
		    </constraint>
            <mustSupport value="true"/>
        </element>
        <element id="Observation.value[x].extension:dataAbsentReason">
            <path value="Observation.value[x].extension"/>
            <sliceName value="dataAbsentReason"/>
            <short value="Reason why the data is not present"/>
            <max value="1"/>
            <type>
                <code value="Extension"/>
                <profile value="http://hl7.org/fhir/StructureDefinition/data-absent-reason"/>
            </type>
            <mustSupport value="true"/>
        </element>
        <element id="Observation.note">
            <path value="Observation.note"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.bodySite">
            <path value="Observation.bodySite"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.specimen">
            <path value="Observation.specimen"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.device">
            <path value="Observation.device"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.hasMember">
            <path value="Observation.hasMember"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.derivedFrom">
            <path value="Observation.derivedFrom"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
        <element id="Observation.component">
            <path value="Observation.component"/>
            <min value="0"/>
            <max value="0"/>
            <mustSupport value="false"/>
        </element>
    </differential>
</StructureDefinition>
